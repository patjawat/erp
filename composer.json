{
    "name": "yiisoft/yii2-app-basic",
    "description": "Yii 2 Basic Project Template",
    "keywords": ["yii2", "framework", "basic", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4.0",
        "yiisoft/yii2": "~2.0.46",
        "yiisoft/yii2-bootstrap4": "~2.0.0",
        "yiisoft/yii2-bootstrap5": "*",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "yiisoft/yii2-symfonymailer": "~2.0.3",
        "hail812/yii2-adminlte3": "~1.1",
        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-detail-view": "*",
        "kartik-v/yii2-checkbox-x": "*",
        "kartik-v/yii2-tabs-x": "dev-master",
        "kartik-v/yii2-datecontrol": "dev-master",
        "kartik-v/yii2-date-range": "dev-master",
        "kartik-v/yii2-editors": "dev-master",
        "dektrium/yii2-user": "^0.9.14",
        "dektrium/yii2-rbac": "1.0.0-alpha@dev",
        "mdmsoft/yii2-admin": "~2.0",
        "mdmsoft/yii2-autonumber": "~1.0",
        "kartik-v/yii2-widget-activeform": "dev-master",
        "kartik-v/yii2-krajee-base": "dev-master",
        "kartik-v/yii2-grid": "dev-master",
        "kartik-v/yii2-widget-select2": "dev-master",
        "kartik-v/yii2-widget-datepicker": "dev-master",
        "kartik-v/yii2-editable": "*",
        "kartik-v/yii2-report": "@dev",
        "kartik-v/yii2-icons": "@dev",
        "kartik-v/yii2-widget-growl": "@dev",
        "kartik-v/yii2-widget-alert": "@dev",
        "kartik-v/yii2-bootstrap4-dropdown": "@dev",
        "kartik-v/yii2-slider": "dev-master",
        "kartik-v/yii2-export": "@dev",
        "insolita/yii2-adminlte-widgets": "^3.2",
        "miloschuman/yii2-highcharts-widget": "dev-master",
        "dominus77/yii2-sweetalert2-widget": "~1.3",
        "dixonsatit/yii2-thai-year-formatter": "*",
        "nterms/yii2-mailqueue": "*",
        "warrence/yii2-kartikgii": "dev-master",
        "kartik-v/yii2-bootstrap5-dropdown": "@dev",
        "twbs/bootstrap-icons": "^1.10",
        "coderius/yii2-hit-counter": "dev-master",
        "kartik-v/yii2-ipinfo": "dev-master",
        "alexandernst/yii2-device-detect": "0.0.12",
        "idk/yii2-google-apiclient": "dev-master",
        "google/apiclient": "^2.0@dev",
        "sergmoro1/yii2-google-spreadsheet": "dev-master",
        "yiisoft/yii2-authclient": "^2.2",
        "iamsaint/yii2-datetimepicker": "dev-master",
        "beaten-sect0r/yii2-flatpickr": "*",
        "onmotion/yii2-widget-apexcharts": "dev-master",
        "kartik-v/yii2-tree-manager": "dev-master",
        "kartik-v/yii2-dialog": "dev-master",
        "creocoder/yii2-nested-sets": "dev-master",
        "muhsamsul/yii2-treeimage": "dev-master",
        "ruskid/yii2-csv-importer": "dev-master",
        "yiithings/yii2-dotenv": "dev-master",
        "kartik-v/yii2-validators": "dev-master",
        "nick-denry/yii2-round-switch-column": "^0.1.2",
        "unclead/yii2-multiple-input": "dev-master",
        "phpoffice/phpword": "dev-master",
        "yurkinx/yii2-image": "dev-master",
        "yiisoft/yii2-imagine": "~2.2.0",
        "karatae99/yii2-date-picker-thai-widget" : "~1.0",
        "istvan-ujjmeszaros/bootstrap-duallistbox": "~4.0.0",
        "softark/yii2-dual-listbox": "dev-master",
        "chillerlan/php-qrcode": "dev-main",
        "kartik-v/php-date-formatter": "dev-master",
        "vivekmarakana/yii2-dynamicform": "dev-master"
        
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/verify": "^2.2",
        "symfony/browser-kit": ">=2.7 <=4.2.4"
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "yiisoft/yii2-composer" : true
        },
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "scripts": {
        "post-install-cmd": [
            "yii\\composer\\Installer::postInstall"
        ],
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject",
            "yii\\composer\\Installer::postInstall"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ]
        },
        "yii\\composer\\Installer::postInstall": {
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
